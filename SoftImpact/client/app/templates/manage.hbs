<h1> List of Students </h1>

<div class='container'>
	<table  class='table'>
	<thead>
		<tr>
			<th>Student Id</th>
		    <th>Name</th>
		    <th>Birth</th>
		    <th>Class</th>
		    <th>Year</th>
		    <th>Quarter</th>
		    <th>Math</th>
		    <th>Computer</th>
		    <th>Literature</th>
		</tr>
	</thead>
	<tbody> 
		{{#each model   as |itemon|}}
		<tr>
		  <td>
			{{itemon.studentid}}
		  </td>
		  <td>
			{{itemon.name}}
		  </td>
		  <td>
			{{itemon.birth}}
		  </td>
		  <td>
			{{itemon.stclass}}
		  </td>
		  <td>
			{{itemon.Year}} 
		  </td>
		  <td> 
			{{itemon.Quarter}}
		 </td>
		 <td>
			{{itemon.math}}
		</td>
		<td>
			{{itemon.computer}}
		</td>
		<td>
			{{itemon.literature}}  	
		</td>
		</tr>
		{{/each}}
	</tbody>
	</table>
	{{outlet}}
</div>
